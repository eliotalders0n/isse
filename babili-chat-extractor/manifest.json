{
  "manifest_version": 3,
  "name": "Babili Chat Extractor",
  "description": "Extract messages from WhatsApp, Google Chat, and other platforms for Babili relationship insights",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "downloads",
    "webNavigation"
  ],
  "host_permissions": [
    "https://web.whatsapp.com/*",
    "https://mail.google.com/*",
    "https://chat.google.com/*",
    "https://babili-isse.web.app/*",
    "http://localhost/*",
    "https://*/*"
  ],
  "action": {
    "default_title": "Babili Chat Extractor",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}